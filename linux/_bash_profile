# bash function to backup files
# Usage: backup <file-to-backup>
function backup () {
    newname=$1.`date +%Y%m%d.%H%M.bak`;
    mv $1 $newname;
    echo "Backed up $1 to $newname.";
    cp -p $newname $1;
}

# If ~/.bash_profile exists ~/.profile is not read. I want it to be read.
if [ -r "$HOME/.profile" ]; then
    . "$HOME/.profile"
fi
